{
	"name": "vite-plugin-autolib",
	"description": "Automatically fills out your package.json for both local and published version",
	"version": "0.0.10",
	"license": "MIT",
	"private": false,
	"homepage": "https://www.github.com/AlexAegis/vite-plugin-autolib#readme",
	"keywords": [
		"javascript",
		"js",
		"ts",
		"typescript",
		"vite"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "vite build",
		"lint:es": "eslint --max-warnings=0 --fix .",
		"lint:format": "prettier --check .",
		"test": "vitest --passWithNoTests --coverage --run",
		"typedoc": "typedoc ./src/index.ts --out ./dist/typedoc"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./example": "./static/example.json",
		"./readme": "./readme.md"
	},
	"bin": {
		"reorder-json": "./shims/reorder-json.mjs"
	},
	"dependencies": {
		"@alexaegis/common": "0.0.20",
		"@alexaegis/fs": "0.0.20",
		"@alexaegis/logging": "0.0.20",
		"@alexaegis/workspace-tools": "0.0.20",
		"@npmcli/package-json": "^3.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.0.3",
		"@types/node": "^18.14.1",
		"fs-extra": "^11.1.0",
		"globby": "^13.1.3",
		"kolorist": "^1.7.0",
		"prettier": "^2.8.4",
		"rollup": "^3.17.3",
		"svelte": "^3.55.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vite-plugin-dts": "^2.0.2",
		"vite-plugin-static-copy": "^0.13.1",
		"yargs": "^17.7.1"
	},
	"peerDependencies": {
		"typescript": "^4.9.5"
	},
	"devDependencies": {
		"@schemastore/package": "^0.0.8",
		"@types/fs-extra": "^11.0.1",
		"@types/prettier": "^2.7.2",
		"@types/yargs": "^17.0.22",
		"@vitest/coverage-istanbul": "^0.29.1",
		"vitest": "^0.29.1"
	}
}
