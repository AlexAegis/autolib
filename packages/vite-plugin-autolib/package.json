		"globby": "^13.1.2",
		"fs-extra": "^10.1.0",
		"rollup": "^3.3.0",
		"typescript": "^4.8.4",
		"ts-node": "^10.9.1",
		"@types/node": "^18.11.9",
		"kolorist": "^1.6.0",
		"svelte": "^3.53.1",
		"prettier": "^2.7.1"
	},
	"peerDependencies": {
		"vite": "^3.2.3"
	},
	"devDependencies": {
		"@types/prettier": "^2.7.1",
		"@types/fs-extra": "^9.0.13",
		"@schemastore/package": "^0.0.6"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	}
